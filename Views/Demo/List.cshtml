@model IEnumerable<UniqueAttribute.Models.TestModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Add")
    </p>
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SomeThing)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SomeThing)
                </td>
                <td>
                    @Html.ActionLink("Update", "Update", new { id = item.Id }) |
                </td>
            </tr>
        }

    </table>
</body>
</html>
